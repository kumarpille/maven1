pipeline
{
    agent any
    stages
    {
        stage('continuousdownload'_loan)
        {
            steps
            {
              git 'https://github.com/intelliqittrainings/maven.git'  
            }
        }
        stage('continuousbuild_loan')
        {
            steps
            {
               sh 'mvn package' 
            }
        }
        
    }
}
